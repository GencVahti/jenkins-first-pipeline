pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Hello World, this is build!'
            }
        }
        stage('push') {
            steps {
                echo 'Hello World, this is push!'
            }
        }
        stage('deploy') {
            steps {
                echo 'Hello World, this is deploy!'
            }
        }
    }
}
